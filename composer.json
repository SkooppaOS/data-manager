{
  "name": "michaels/data-manager",
  "description": "Simple data manager for deep data structures, access via dot notation access, and raw or prepared retrieval",
  "keywords": [
    "data", "collection", "container", "manager"
  ],
  "homepage": "https://github.com/chrismichaels84/data-manager",
  "license": "MIT",
  "authors": [
    {
      "name": "Michael",
      "email": "phoenixlabsdev@gmail.com",
      "homepage": "http://phoenixlabstech.org",
      "role": "Lead Developer"
    }
  ],
  "require": {
    "php" : ">=5.4.0",
    "container-interop/container-interop": "1.1.0"
  },
  "require-dev": {
    "phpunit/phpunit" : "4.*",
    "satooshi/php-coveralls": "dev-master",
    "codeception/specify": "*"
  },
  "suggests": {
    "illuminate/support": "5.0.x-dev"
  },
  "autoload": {
    "psr-4": {
      "Michaels\\Manager\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Michaels\\Manager\\Test\\": "tests"
    }
  }
}
